          },
          "telecom": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TelecomFHIRObject"
            }
          },
          "gender": {
            "type": "string",
            "enum": [
              "male",
              "female",
              "other",
              "unknown"
            ]
          },
          "birthDate": {
            "type": "string",
            "format": "date"
          },
          "address": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AddressFHIRObject"
            }
          },
          "maritalStatus": {

          "extension": [
            {
              "url": "https://converge.amwell.systems/visitType",
              "valueCode": "Initial"
            }
          ],
          "participant": [
            {
              "type": [
                {
                  "coding": [
                    {
                      "system": "http://terminology.hl7.org/CodeSystem/v3-ParticipationType",
                      "code": "PPRF"
                    }
                  ]
                }
              ],
              "actor": {
                "identifier": {
                  "system": "https://converge.amwell.systems/NPI",
                  "value": "12345"
                }
              },
              "status": "accepted"
            },
            {
              "actor": {
                "identifier": {
                  "system": "https://converge.amwell.systems/MRN",

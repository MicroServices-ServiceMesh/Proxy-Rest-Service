            "type": "string"
          },
          "code": {
            "type": "string"
          }
        }
      },
      "ReferenceFHIRObject": {
        "type": "object",
        "properties": {
          "reference": {
            "type": "string",
            "example": "HealthcareService/ae709cfb-fec1-4ce8-b584-ebc1fb4d888d"
          },
          "type": {
            "type": "string"
          },
          "identifier": {
            "$ref": "#/components/schemas/IdentifierFHIRObject"
          },
          "display": {
            "type": "string"
          }
        }
      },
      "OperationOutcomeSuccess": {
        "type": "object",
        "description": "FHIR OperationOutcome",
        "example": {
          "resourceType": "OperationOutcome",
          "issue": [
            {
              "severity": "information",
              "code": "informational",
              "details": {
                "text": "All OK"
              }
            }
          ]
        }
      },
      "OperationOutcomeError": {
        "type": "object",
        "description": "FHIR OperationOutcome",
        "example": {
          "resourceType": "OperationOutcome",
          "id": "101",
          "issue": [
            {
              "severity": "error",
              "code": "structure",
              "details": {
                "text": "Practitioner resource is missing name"
              },
              "location": [
                "/f:Practitioner/f:name"
              ],
              "expression": [
                "Patient.name"
              ]
            }
          ]
        }
      },

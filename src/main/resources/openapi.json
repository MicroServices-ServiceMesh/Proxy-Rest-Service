      "AppointmentFHIRObject": {
        "type": "object",
        "description": "FHIR Appointment",
        "required": [
          "resourceType",
          "status",
          "start",
          "end",
          "extension",
          "participant"
        ],
        "properties": {
          "resourceType": {
            "type": "string",
            "example": "Appointment"
          },
          "status": {
            "type": "string",
            "enum": [
              "proposed",
              "pending",
              "booked",
              "arrived",
              "fulfilled",
              "cancelled",
              "noshow",
              "entered-in-error",
              "checked-in",
              "waitlist"
            ],
            "example": "booked"
          },
          "serviceCategory": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CodeableConceptFHIRObject"
            }
          },
          "serviceType": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CodeableConceptFHIRObject"
            }
          },
          "specialty": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CodeableConceptFHIRObject"
            }
          },
          "appointmentType": {
            "$ref": "#/components/schemas/CodeableConceptFHIRObject"
          },
          "reasonCode": {
            "$ref": "#/components/schemas/CodeableConceptFHIRObject"
          },
          "reasonReference": {
            "$ref": "#/components/schemas/ReferenceFHIRObject"
          },
          "priority": {
            "type": "integer"
          },
          "description": {
            "type": "string"
          },
          "start": {
            "type": "string",
            "format": "date-time",
            "example": "2023-05-09T10:45:00.000+00:00"
          },
          "end": {
            "type": "string",
            "format": "date-time",
            "example": "2023-05-09T09:00:00.000+00:00"
          },
          "slot": {
            "$ref": "#/components/schemas/ReferenceFHIRObject"
          },
          "created": {
            "type": "string",
            "format": "date-time"
          },
          "comment": {
            "type": "string"
          },
          "patientInstruction": {
            "type": "string"
          },
          "extension": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ExtensionFHIRObject"
            }
          },
          "participant": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ParticipantFHIRObject"
            }
          }
        },
